" Options
set devtoolsposition=split
set downloadmethod=ask
set favoritepages=https://start.duckduckgo.com/?kae=d&kav=1&ko=-1&k1=-1&ia=web
set permissioncertificateerror=ask
set searchengine=https://start.duckduckgo.com/?kae=d&kav=1&ko=-1&k1=-1&q=%s&ia=web
set splitbelow
set splitright
set tabreopenposition=previous

" Mappings
map <M-q> <:quitall>
map <M-w> <:close>

" vim: ft=vim
